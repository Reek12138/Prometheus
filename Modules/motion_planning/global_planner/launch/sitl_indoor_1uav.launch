<launch>
	<!-- 无人机ID -->
	<arg name="uav_id" default="1"/>
	<include file="$(find prometheus_gazebo)/launch_basic/sitl_px4_indoor.launch">
		<arg name="uav_id" value="$(arg uav_id)"/>
		<arg name="sdf" value="$(find prometheus_gazebo)/gazebo_models/uav_models/p230/p230.sdf"/>
		<arg name="model" value="p230"/>
        <arg name="uav_init_x" value="-10.0"/>
        <arg name="uav_init_y" value="0.0"/>
        <arg name="uav_init_z" value="0.15"/>
        <arg name="uav_init_yaw" value="0.0"/>
	</include>

    <!-- map_generator -->
    <include file="$(find prometheus_simulator_utils)/launch/map_generator.launch">
    </include>

	<!-- uav_control_main -->
    <include file="$(find prometheus_uav_control)/launch/uav_control_main_indoor.launch">
    </include>

</launch>

